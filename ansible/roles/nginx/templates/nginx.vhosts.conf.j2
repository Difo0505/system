server {

{% for k,v in item.iteritems() %}
{% if k.find('location') == -1 and k != "name" and k != "log_dir" %}
  {{ k }} {{ v }};
{% endif %}
{% endfor %}

{% for k,v in item.iteritems() %}
{% if k.find('location') != -1 %}
  location {{ v.name }} {
{% for x,y in v.iteritems() if x != 'name' %}
    {{ x }} {{ y }};
{% endfor %}
  }

{% endif %}
{% endfor %}
}
